{
  
    "variables": {
      "default_format": "\\medskip\\mydefaultsize",
      "section_header": "\\section*{#CONTENT#}",
      "subsection_header": "\\subsection*{#CONTENT#}"
    },
    "commands": {
      "problem": {
        "description": "Problem section",
        "parameters": {
          "title": {
            "description": "Problem title",
            "default": ""
          }
        },
        "latex_template": "$variables.default_format$\n\\section*{#title#}\n#CONTENT#\\par"
      },
      "text": {
        "description": "Text block",
        "parameters": {},
        "latex_template": "$variables.default_format$\n#CONTENT#\\par"
      },
      "question": {
        "description": "Question text",
        "parameters": {},
        "latex_template": "$variables.default_format$\n#CONTENT#\\par"
      },
      "bullet": {
        "description": "Bullet point item",
        "parameters": {},
        "latex_template": "$variables.default_format$\n\\begin{itemize}\n\\item #CONTENT#\n\\end{itemize}"
      },
      "enum": {
        "description": "Enumerated list item",
        "parameters": {},
        "latex_template": "$variables.default_format$\n\\item #CONTENT#"
      },
      "image": {
        "description": "Image insertion",
        "parameters": {
          "file": {
            "description": "Image filename",
            "type": "string",
            "required": true
          },
          "width": {
            "description": "Image width as fraction of text width",
            "type": "number",
            "default": 0.8,
            "min": 0.1,
            "max": 1.0
          },
          "caption": {
            "description": "Image caption",
            "type": "string",
            "default": ""
          },
          "label": {
            "description": "Reference label",
            "type": "string",
            "default": ""
          }
        },
        "latex_template": "\\begin{figure}[htbp]\n\\centering\n\\includegraphics[width=#width#\\textwidth]{#file#}\n#IF caption!=\"\"#\\caption{#caption#}#ENDIF#\n#IF label!=\"\"#\\label{#label#}#ENDIF#\n\\end{figure}"
      },
      "eq": {
        "description": "Equation block",
        "parameters": {},
        "latex_template": "\\[\n#CONTENT#\n\\]"
      },
      "nl": {
        "description": "Newline (line break)",
        "parameters": {},
        "latex_template": "\\\\"
      },
      "answer": {
        "description": "Answer section",
        "parameters": {
          "color": {
            "description": "Answer color",
            "type": "string",
            "default": "red"
          }
        },
        "latex_template": "$variables.default_format$\n\\textcolor{#color#}{\\textbf{Answer:} #CONTENT#}\\par"
      },
      "solution": {
        "description": "Solution section",
        "parameters": {},
        "latex_template": "\\textbf{Solution:}\n#CONTENT#\\par"
      },
      "align": {
        "description": "Aligned equations block",
        "parameters": {},
        "latex_template": "\\begin{align*}\n#CONTENT#\n\\end{align*}"
      },
      "box": {
        "description": "Colored box",
        "parameters": {
          "color": {
            "description": "Background color",
            "default": "white"
          }
        },
        "latex_template": "\\begin{mdframed}[backgroundcolor=#color#]\n#CONTENT#\n\\end{mdframed}"
      },
      "table": {
        "description": "Simple table",
        "parameters": {
          "columns": {
            "description": "Column alignment",
            "type": "string",
            "default": "lll"
          },
          "caption": {
            "description": "Table caption",
            "type": "string",
            "default": ""
          },
          "label": {
            "description": "Reference label",
            "type": "string",
            "default": ""
          }
        },
        "latex_template": "\\begin{table}[htbp]\n\\centering\n\\begin{tabular}{#columns#}\n#CONTENT#\n\\end{tabular}\n#IF caption!=\"\"#\\caption{#caption#}#ENDIF#\n#IF label!=\"\"#\\label{#label#}#ENDIF#\n\\end{table}"
      },
      "config": {
        "description": "Document-level configuration",
        "parameters": {},
        "latex_template": ""
      }
    }
  }